{
  "name": "automated-blog-poster-infrastructure",
  "version": "1.0.0",
  "bin": {
    "infrastructure": "bin/infrastructure.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "lint": "eslint . --ext ts --report-unused-disable-directives --max-warnings 0",
    "test": "jest",
    "test:unit": "jest --testPathPattern=.*-unit\\.test\\.ts",
    "test:integration": "jest --testPathPattern=.*-integration\\.test\\.ts",
    "test:load": "jest --testPathPattern=load-testing\\.test\\.ts --testTimeout=300000",
    "test:security": "jest --testPathPattern=security\\.test\\.ts",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "cdk": "cdk",
    "deploy": "cdk deploy",
    "destroy": "cdk destroy",
    "diff": "cdk diff",
    "synth": "cdk synth",
    "rollback": "cdk deploy --rollback"
  },
  "devDependencies": {
    "@aws-sdk/client-cloudwatch": "^3.882.0",
    "@aws-sdk/client-sns": "^3.882.0",
    "@types/aws-lambda": "^8.10.152",
    "@types/jest": "^29.4.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "20.1.0",
    "@types/node-fetch": "^2.6.13",
    "@types/uuid": "^9.0.8",
    "aws-cdk": "^2.1025.0",
    "aws-sdk-client-mock": "^4.1.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "~5.0.4"
  },
  "dependencies": {
    "@aws-cdk/aws-bedrock-alpha": "^2.211.0-alpha.0",
    "@aws-sdk/client-bedrock-agent-runtime": "^3.864.0",
    "@aws-sdk/client-dynamodb": "^3.450.0",
    "@aws-sdk/client-eventbridge": "^3.450.0",
    "@aws-sdk/client-s3": "^3.450.0",
    "@aws-sdk/client-secrets-manager": "^3.879.0",
    "@aws-sdk/client-sqs": "^3.450.0",
    "@aws-sdk/client-transcribe": "^3.450.0",
    "@aws-sdk/lib-dynamodb": "^3.868.0",
    "aws-cdk-lib": "^2.211.0",
    "constructs": "^10.0.0",
    "jsonwebtoken": "^9.0.2",
    "node-fetch": "^3.3.2",
    "sharp": "^0.33.5",
    "uuid": "^9.0.1"
  }
}
